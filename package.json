{
  "name": "@jupyterlab/google-drive",
  "version": "0.4.0",
  "description": "Realtime collaboration with JupyterLab through Google Drive",
  "author": "Ian Rose",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/*/*d.ts",
    "lib/*/*.js",
    "lib/*.d.ts",
    "lib/*.js",
    "style/*.*"
  ],
  "directories": {
    "lib": "lib/"
  },
  "keywords": [
    "jupyter",
    "jupyterlab",
    "google drive"
  ],
  "jupyterlab": {
    "extension": true
  },
  "scripts": {
    "build": "tsc",
    "build:test": "tsc --project test && webpack --config test/webpack.config.js",
    "test": "karma start --browsers=Firefox test/karma.conf.js",
    "clean": "rimraf lib",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@jupyterlab/application": "^0.8.0",
    "@jupyterlab/apputils": "^0.8.0",
    "@jupyterlab/coreutils": "^0.8.0",
    "@jupyterlab/docmanager": "^0.8.0",
    "@jupyterlab/docregistry": "^0.8.0",
    "@jupyterlab/filebrowser": "^0.8.0",
    "@jupyterlab/services": "^0.47.0",
    "@phosphor/algorithm": "^1.1.1",
    "@phosphor/application": "^1.3.0",
    "@phosphor/commands": "^1.3.0",
    "@phosphor/coreutils": "^1.2.0",
    "@phosphor/disposable": "^1.1.1",
    "@phosphor/signaling": "^1.2.1",
    "@phosphor/widgets": "^1.3.0",
    "core-js": "^2.4.1",
    "es6-promise": "^4.1.0",
    "jquery": "^2.2.0",
    "mime-types": "^2.1.15"
  },
  "devDependencies": {
    "@types/expect.js": "^0.3.29",
    "@types/google-drive-realtime-api": "^0.0.33",
    "@types/jquery": "^2.0.32",
    "@types/mime-types": "^2.1.0",
    "@types/mocha": "^2.2.32",
    "@types/node": "^7.0.11",
    "expect.js": "^0.3.1",
    "karma": "^1.5.0",
    "karma-chrome-launcher": "^0.2.2",
    "karma-firefox-launcher": "^0.1.7",
    "karma-mocha": "^0.2.1",
    "karma-mocha-reporter": "^1.1.5",
    "karma-sourcemap-loader": "^0.3.7",
    "mocha": "^3.2.0",
    "rimraf": "^2.5.2",
    "typescript": "~2.4.1",
    "webpack": "^2.2.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jupyterlab/jupyterlab-google-drive.git"
  },
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/jupyterlab/jupyterlab-google-drive/issues"
  },
  "homepage": "https://github.com/jupyterlab/jupyterlab-google-drive"
}
