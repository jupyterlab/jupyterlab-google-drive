{
  "name": "@jupyterlab/google-drive",
  "version": "2.0.0",
  "description": "Cloud storage with JupyterLab through Google Drive",
  "keywords": [
    "google drive",
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/jupyterlab/jupyterlab-google-drive",
  "bugs": {
    "url": "https://github.com/jupyterlab/jupyterlab-google-drive/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jupyterlab/jupyterlab-google-drive.git"
  },
  "license": "BSD-3-Clause",
  "author": "Ian Rose",
  "files": [
    "lib/*/*d.ts",
    "lib/*/*.js",
    "lib/*.d.ts",
    "lib/*.js",
    "schema/*.json",
    "style/*.*"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "directories": {
    "lib": "lib/"
  },
  "scripts": {
    "build": "tsc",
    "build:test": "cd test && ./build-tests.sh",
    "clean": "rimraf lib",
    "precommit": "lint-staged",
    "prettier": "prettier --write '**/*{.ts,.tsx,.js,.jsx,.css,.json,.md}'",
    "test": "cd test && ./run-tests.sh",
    "watch": "tsc -w"
  },
  "lint-staged": {
    "**/*{.ts,.tsx,.css,.json,.md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@jupyterlab/application": "^2.0.0",
    "@jupyterlab/apputils": "^2.0.0",
    "@jupyterlab/coreutils": "^4.0.0",
    "@jupyterlab/docmanager": "^2.0.0",
    "@jupyterlab/docregistry": "^2.0.0",
    "@jupyterlab/filebrowser": "^2.0.0",
    "@jupyterlab/mainmenu": "^2.0.0",
    "@jupyterlab/observables": "^3.0.0",
    "@jupyterlab/services": "^5.0.0",
    "@jupyterlab/settingregistry": "^2.0.0",
    "@lumino/algorithm": "^1.2.3",
    "@lumino/commands": "^1.10.1",
    "@lumino/coreutils": "^1.4.2",
    "@lumino/signaling": "^1.3.5",
    "@lumino/widgets": "^1.11.1"
  },
  "devDependencies": {
    "@types/expect.js": "^0.3.29",
    "@types/gapi": "^0.0.36",
    "@types/gapi.auth2": "^0.0.50",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.0.10",
    "css-loader": "^3.0.0",
    "expect.js": "^0.3.1",
    "file-loader": "^4.0.0",
    "husky": "^2.4.1",
    "karma": "^6.3.16",
    "karma-chrome-launcher": "^2.2.0",
    "karma-firefox-launcher": "^1.1.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sourcemap-loader": "^0.3.7",
    "lint-staged": "^8.2.1",
    "mocha": "^6.1.4",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.3",
    "style-loader": "^0.23.1",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.5.2",
    "url-loader": "^2.0.0",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.11"
  },
  "jupyterlab": {
    "extension": true,
    "schemaDir": "schema"
  }
}
